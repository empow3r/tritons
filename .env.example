# Tritons Multi-Agent Orchestration System
# Environment Variables Configuration Example
# 
# Copy this file to .env and fill in your actual API keys and credentials
# NEVER commit the .env file to version control

# Primary LLM API Keys
CLAUDE_API_KEY=your-claude-api-key-here
CLAUDE_API_KEY2=your-claude-api-key2-here
CLAUDE_API_KEY3=your-claude-api-key3-here

OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_KEY2=your-openai-api-key2-here
OPENAI_API_KEY3=your-openai-api-key3-here

# Additional LLM Providers
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_API_KEY2=your-gemini-api-key2-here

DEEPSEEK_API_KEYS=your-deepseek-api-key-here
DEEPSEEK_API_KEY2=your-deepseek-api-key2-here
DEEPSEEK_API_KEY3=your-deepseek-api-key3-here
DEEPSEEK_API_KEY4=your-deepseek-api-key4-here

MISTRAL_API_KEY=your-mistral-api-key-here
KIMI_API_KEY=your-kimi-api-key-here

OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_API_KEY2=your-openrouter-api-key2-here

# Proxy Configuration (optional)
WEBSHARE_PROXY_URL=your-webshare-proxy-url-here

# Database Passwords (change these for production)
REDIS_PASSWORD=change-this-password
POSTGRES_PASSWORD=change-this-password
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=change-this-password
GRAFANA_PASSWORD=change-this-password

# Rate Limiting Configuration
MAX_REQUESTS_PER_MINUTE_PER_KEY=50
MAX_REQUESTS_PER_MINUTE_PER_IP=100
REQUEST_TIMEOUT=30000
RETRY_ATTEMPTS=5

# Feature Flags
ENABLE_PROXY_ROTATION=true
ENABLE_REQUEST_THROTTLING=true
ENABLE_IP_ROTATION=true
ENABLE_USER_AGENT_ROTATION=true